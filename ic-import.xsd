   <xs:simpleType name="referenceLength">
      <xs:annotation>
         <xs:documentation xml:lang="en">Reference content must be 25 to 5000 characters long</xs:documentation>
         <xs:documentation xml:lang="pl">Przypis bibliograficzny musi posiadać od 25 do 5000 znaków</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:minLength value="25" />
         <xs:maxLength value="5000" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="pageNumber">
      <xs:restriction base="xs:integer">
         <xs:minInclusive value="0" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="doiType">
      <xs:annotation>
         <xs:documentation xml:lang="en">DOI number</xs:documentation>
         <xs:documentation xml:lang="pl">Numer DOI</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:pattern value="(10[.][0-9]{4,}([.][0-9]+)*/([a-zA-Z0-9()]*)([.|\-/][a-zA-Z0-9()]*)*)" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="issnType">
      <xs:restriction base="xs:string">
         <xs:minLength value="8" />
         <xs:maxLength value="9" />
         <xs:pattern value="\d{4}-?\d{3}[\dX]" />
      </xs:restriction>
   </xs:simpleType>
