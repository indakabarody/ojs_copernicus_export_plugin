   <xs:element name="ici-import">
      <xs:annotation>
         <xs:documentation xml:lang="en">Top level element for a issues and content metadata submission to ICI World of Journals. This element indicates start and end of the XML file.</xs:documentation>
         <xs:documentation xml:lang="pl">Element najwyższego poziomu dla metadanych wydań i zawartości wdrywanych do ICI World of Journals. Ten element wskazuje początek i koniec pliku XML.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="journal" nillable="false">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The container for all information about a journal for which we want to import issues and contents.</xs:documentation>
                  <xs:documentation xml:lang="pl">Kontener zawierający metadane czasopisma dla którego importujemy wydania i zawartość</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:attribute name="issn" use="required" type="issnType">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">ISSN number of the journal</xs:documentation>
                        <xs:documentation xml:lang="pl">Numer ISSN czasopisma</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:complexType>
            </xs:element>
            <xs:element name="issue" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The container for metadata that defines a single issue of a journal. If you want to import several issues, use several 'issue' containers.</xs:documentation>
                  <xs:documentation xml:lang="pl">Kontener zawierający metadane pojedyńczego wydania czasopisma. W przypadku importu kilku wydań, użyj kilku kontenerów 'issue'.</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="article" maxOccurs="unbounded">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">The container for all information about a single journal article. A journal article is required to have at least one language version, one author and article type.</xs:documentation>
                           <xs:documentation xml:lang="pl">Kontener zawierający informacje o pojedyńczym artykule. Powinien zawierać przyjajmniej jedną wersję językową, jednego autora i określenie typu artykułu.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                           <xs:sequence>
                              <xs:element name="type" type="articleType" minOccurs="0">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Publication type. Please use only values from the list.</xs:documentation>
                                    <xs:documentation xml:lang="pl">Typ artykułu. Zgodny z listą.</xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="languageVersion" maxOccurs="unbounded">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">The container for all information about a single language version of the publication. Each publication must contain at least one language version. The language versions of the publication, in the ICI World of Journals system, will be presented as separate publications.</xs:documentation>
                                    <xs:documentation xml:lang="pl">Kontener zawierący informacje o pojedyńczej wersji językowej publikacji. Każda publikacja musi zawierać co najmniej jedną wersję językową. Wersje językowe publikacji, w systemie ICI World of Journals będą prezentoane jako oddzielne publikacje.</xs:documentation>
                                 </xs:annotation>
                                 <xs:complexType>
                                    <xs:sequence>
                                       <xs:element name="title" type="xs:string" nillable="false">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">Title of the publication.</xs:documentation>
                                             <xs:documentation xml:lang="pl">Tytuł publikacji.</xs:documentation>
                                          </xs:annotation>
                                       </xs:element>
                                       <xs:element name="abstract" type="xs:string" nillable="false">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">Abstract of the publication.</xs:documentation>
                                             <xs:documentation xml:lang="pl">Abstrakt publikacji.</xs:documentation>
                                          </xs:annotation>
                                       </xs:element>
                                       <xs:element name="pdfFileUrl" type="xs:anyURI" minOccurs="0">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">Adres URL do pliku z pełną, elektroniczną wersją publikacji. Dopuszczalny format to PDF. Plik będzie widoczny dla wszystkich w systemie ICI WOJ.</xs:documentation>
                                             <xs:documentation xml:lang="pl">URL to a file with an electronic version of the publication. Permitted formats are PDF. The file will WILL BE shared publicly.</xs:documentation>
                                          </xs:annotation>
                                       </xs:element>
                                       <xs:element name="publicationDate" type="xs:date" minOccurs="0">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">Publication date.</xs:documentation>
                                             <xs:documentation>Data publikacji.</xs:documentation>
                                          </xs:annotation>
                                       </xs:element>
                                       <xs:element name="registrationDate" type="xs:dateTime" minOccurs="0" />
                                       <xs:element name="pageFrom" type="pageNumber" minOccurs="0" maxOccurs="1">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">First page of the publication.</xs:documentation>
                                             <xs:documentation xml:lang="pl">Numer pierwszej strony publikacji.</xs:documentation>
                                          </xs:annotation>
                                       </xs:element>
                                       <xs:element name="pageTo" type="pageNumber" minOccurs="0" maxOccurs="1">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">Last page of the publication.</xs:documentation>
                                             <xs:documentation xml:lang="pl">Ostatnia strona publikacji.</xs:documentation>
                                          </xs:annotation>
                                       </xs:element>
                                       <xs:element name="doi" type="doiType" minOccurs="0" maxOccurs="1">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">DOI number of the publication.</xs:documentation>
                                             <xs:documentation>Numer DOI publikacji.</xs:documentation>
                                          </xs:annotation>
                                       </xs:element>
                                       <xs:element name="keywords" minOccurs="0">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">A container for keywords related to the language version of the publication. Must contain at least one keyword.</xs:documentation>
                                             <xs:documentation xml:lang="pl">Kontener dla słów kluczowych związanych z wersją językową publikacji. Musi zawierać co namjniej jedno słowo kluczowe.</xs:documentation>
                                          </xs:annotation>
                                          <xs:complexType>
                                             <xs:sequence>
                                                <xs:element name="keyword" type="xs:string" maxOccurs="unbounded">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">A single keyword related to the language version of the publication.</xs:documentation>
                                                      <xs:documentation xml:lang="pl">Pojedyńcze słowo kluczowe związane z wersją językową publikacji.</xs:documentation>
                                                   </xs:annotation>
                                                </xs:element>
                                             </xs:sequence>
                                          </xs:complexType>
                                       </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="externalId" type="xs:string" use="optional">
                                       <xs:annotation>
                                          <xs:documentation xml:lang="en">A publisher identifier that can be used to uniquely identify the language version of the publication being registered. This identifier is a publisher-assigned number that uniquely identifies the language version of the publication.</xs:documentation>
                                          <xs:documentation xml:lang="pl">Unikalny identyfikator wersji językowej publikacji, nadany przez wydawcę. Służy do jednoznacznego określenia wersji językowej publikacji w systemie wydawcy</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="language" type="xs:string" use="required">
                                       <xs:annotation>
                                          <xs:documentation xml:lang="en">Publication language code according to ISO 639. Default value: PL</xs:documentation>
                                          <xs:documentation xml:lang="pl">Kod języka publikacji zgodny z ISO 639. Domyślnie wartość PL</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                 </xs:complexType>
                              </xs:element>
                              <xs:element name="authors">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">The container for all information about the contributors/authors of the publication. Must contain at least one contributor/author.</xs:documentation>
                                    <xs:documentation xml:lang="pl">Kontener zawierający informacje o wszystkich autorach publikacji. Musi zawierać informacje o co najmniej jednym autorze.</xs:documentation>
                                 </xs:annotation>
                                 <xs:complexType>
                                    <xs:sequence>
                                       <xs:element name="author" maxOccurs="unbounded">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">The container for all information about singe contributor/author of the publication.</xs:documentation>
                                             <xs:documentation xml:lang="pl">Kontener zawierający informacje o pojedyńczym autorze publikacji.</xs:documentation>
                                          </xs:annotation>
                                          <xs:complexType>
                                             <xs:sequence>
                                                <xs:element name="name" nillable="false">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">First name of the contributor/author. May be submitted as either a full first name or initials. Do not include titles such as "Prof.", "Dr." or other.</xs:documentation>
                                                      <xs:documentation>Imię autora. Może być przesłane jako pełne imię lub inicjały. Nie zamieszczaj tytułów takich jak "Prof.", "Dr." lub innych.</xs:documentation>
                                                   </xs:annotation>
                                                   <xs:simpleType>
                                                      <xs:restriction base="xs:string">
                                                         <xs:maxLength value="150" />
                                                      </xs:restriction>
                                                   </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="name2" minOccurs="0">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">Second name of the contributor/author.</xs:documentation>
                                                      <xs:documentation xml:lang="pl">Drugie imię autora.</xs:documentation>
                                                   </xs:annotation>
                                                   <xs:simpleType>
                                                      <xs:restriction base="xs:string">
                                                         <xs:maxLength value="150" />
                                                      </xs:restriction>
                                                   </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="surname" nillable="false">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">Surname of the contributor/author.</xs:documentation>
                                                      <xs:documentation xml:lang="pl">Nazwisko autora</xs:documentation>
                                                   </xs:annotation>
                                                   <xs:simpleType>
                                                      <xs:restriction base="xs:string">
                                                         <xs:maxLength value="150" />
                                                      </xs:restriction>
                                                   </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="email" minOccurs="0">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">Email of the contributor/author.</xs:documentation>
                                                      <xs:documentation xml:lang="pl">Email autora.</xs:documentation>
                                                   </xs:annotation>
                                                   <xs:simpleType>
                                                      <xs:restriction base="xs:string">
                                                         <xs:maxLength value="255" />
                                                      </xs:restriction>
                                                   </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="polishAffiliation" type="xs:boolean" minOccurs="0">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="EN">Does the institution to which the author affiliates is Polish.</xs:documentation>
                                                      <xs:documentation xml:lang="PL">Czy instytucja do której afiliuje autor jest Polska.</xs:documentation>
                                                   </xs:annotation>
                                                </xs:element>
                                                <xs:element name="order" type="xs:integer">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">Author's sequence/order number.</xs:documentation>
                                                      <xs:documentation xml:lang="pl">Numer kolejny autora</xs:documentation>
                                                   </xs:annotation>
                                                </xs:element>
                                                <xs:element name="instituteAffiliation" minOccurs="0">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">The institution with which a contributor/author is affiliated. This element may hold the name and location of an affiliation with which a contributor/author is affiliated.</xs:documentation>
                                                      <xs:documentation xml:lang="pl">Instutycja do której afiliuje autor.</xs:documentation>
                                                   </xs:annotation>
                                                   <xs:simpleType>
                                                      <xs:restriction base="xs:string">
                                                         <xs:maxLength value="255" />
                                                      </xs:restriction>
                                                   </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="departmentAffiliation" minOccurs="0">
                                                   <xs:simpleType>
                                                      <xs:restriction base="xs:string">
                                                         <xs:maxLength value="255" />
                                                      </xs:restriction>
                                                   </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="subjectAffiliation" minOccurs="0">
                                                   <xs:simpleType>
                                                      <xs:restriction base="xs:string">
                                                         <xs:maxLength value="255" />
                                                      </xs:restriction>
                                                   </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="role" type="roleType" minOccurs="0">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">Role of the contributor/author. Please use only values from the list.</xs:documentation>
                                                      <xs:documentation>Rola w powstaniu publikacji. Proszę używać tylko wartości z listy.</xs:documentation>
                                                   </xs:annotation>
                                                </xs:element>
                                                <xs:element name="ORCID" minOccurs="0">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="EN">The ORCID for an contributor/author. Acceptable format: https://orcid.org/0000-0000-0000-0000</xs:documentation>
                                                      <xs:documentation xml:lang="PL">Numer ORCID autora w formacie: https://orcid.org/0000-0000-0000-0000</xs:documentation>
                                                   </xs:annotation>
                                                   <xs:simpleType>
                                                      <xs:restriction base="xs:string">
                                                         <xs:pattern value="https?://orcid.org/[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{3}[X0-9]{1}" />
                                                      </xs:restriction>
                                                   </xs:simpleType>
                                                </xs:element>
                                             </xs:sequence>
                                          </xs:complexType>
                                       </xs:element>
                                    </xs:sequence>
                                 </xs:complexType>
                              </xs:element>
                              <xs:element name="references" minOccurs="0">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">The container for all references/citations from the publication.</xs:documentation>
                                    <xs:documentation xml:lang="pl">Kontener zawierający wszystkie referencje/cytowania z publikacji.</xs:documentation>
                                 </xs:annotation>
                                 <xs:complexType>
                                    <xs:sequence>
                                       <xs:element name="reference" maxOccurs="unbounded">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">The container for all information about single reference/citation from publication.</xs:documentation>
                                             <xs:documentation xml:lang="pl">Kontener zawierający wszystkie informacje o referencji/cytowaniu występującym w publikacji.</xs:documentation>
                                          </xs:annotation>
                                          <xs:complexType>
                                             <xs:sequence>
                                                <xs:element name="unparsedContent" type="referenceLength" nillable="false" minOccurs="1" maxOccurs="1">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">Reference/citation content string.</xs:documentation>
                                                      <xs:documentation xml:lang="pl">Treść referencji/cytowania.</xs:documentation>
                                                   </xs:annotation>
                                                </xs:element>
                                                <xs:element name="order" type="xs:integer" nillable="false">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="en">Reference/citation sequence number.</xs:documentation>
                                                      <xs:documentation xml:lang="pl">Numer kolejny referencji/cytowania.</xs:documentation>
                                                   </xs:annotation>
                                                </xs:element>
                                                <xs:element name="doi" type="doiType" minOccurs="0" maxOccurs="1">
                                                   <xs:annotation>
                                                      <xs:documentation xml:lang="EN">Reference/citation DOI number.</xs:documentation>
                                                      <xs:documentation xml:lang="PL">Numer DOI referencji/cytowanej publikacji.</xs:documentation>
                                                   </xs:annotation>
                                                </xs:element>
                                             </xs:sequence>
                                          </xs:complexType>
                                       </xs:element>
                                    </xs:sequence>
                                 </xs:complexType>
                              </xs:element>
                              <xs:element name="disciplineSciences" minOccurs="0">
                                 <xs:complexType>
                                    <xs:sequence>
                                       <xs:element name="areaScience" type="areaScienceType" maxOccurs="unbounded" />
                                       <xs:element name="fieldScience" type="fieldScienceType" maxOccurs="unbounded" />
                                       <xs:element name="disciplineScience" type="disciplineScienceType" minOccurs="0" maxOccurs="unbounded" />
                                    </xs:sequence>
                                 </xs:complexType>
                              </xs:element>
                           </xs:sequence>
                           <xs:attribute name="icid" type="xs:integer" use="optional">
                              <xs:annotation>
                                 <xs:documentation xml:lang="en">Publication's ICID identifier.</xs:documentation>
                                 <xs:documentation xml:lang="pl">Identyfikator ICID publikacji.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="gicid" type="xs:string" use="optional">
                              <xs:annotation>
                                 <xs:documentation xml:lang="en">Publication's GICID identifier.</xs:documentation>
                                 <xs:documentation xml:lang="pl">Identyfikator GICID publikacji</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="externalId" type="xs:string" use="optional">
                              <xs:annotation>
                                 <xs:documentation xml:lang="en">A publisher identifier that can be used to uniquely identify the publication being registered. This identifier is a publisher-assigned number that uniquely identifies the publication.</xs:documentation>
                                 <xs:documentation xml:lang="pl">Unikalny identyfikator publikacji, nadany przez wydawcę. Służy do jednoznacznego określenia publikacji w systemie wydawcy</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                        </xs:complexType>
                     </xs:element>
                  </xs:sequence>
                  <xs:attribute name="number" type="xs:string" use="required">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">The issue number.</xs:documentation>
                        <xs:documentation xml:lang="pl">Numer wydania</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="volume" type="xs:string" use="required">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">The volume number</xs:documentation>
                        <xs:documentation xml:lang="pl">Tom wydania</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="year" type="xs:integer" use="required">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Publication year</xs:documentation>
                        <xs:documentation xml:lang="pl">Rok wydania</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="publicationDate" type="xs:date" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Publication date</xs:documentation>
                        <xs:documentation xml:lang="pl">Data publikacji</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="coverDate" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Date from the cover</xs:documentation>
                        <xs:documentation xml:lang="pl">Data z okładki</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="coverUrl" type="xs:anyURI" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">URL to the image with the cover of the issue. Acceptable image formats are JPEG and PNG. The maximum width of the image is 200px.</xs:documentation>
                        <xs:documentation xml:lang="pl">Adres URL do obrazka z okładką wydania. Dopuszczalne formaty obrazka to JPEG i PNG. Maksymalna szerokość obrazkato 200px.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="electronicContentPdfUrl" type="xs:anyURI" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">URL to a file with an electronic version of the edition. Permitted formats are PDF. The file will NOT BE shared publicly.</xs:documentation>
                        <xs:documentation xml:lang="pl">Adres URL do pliku z elektroniczną wersją wydania. Dopuszczalny formaty to PDF. Plik NIE BĘDZIE udostępniony publicznie.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="numberOfArticles" type="xs:integer" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Number of scientific articles in this issue.</xs:documentation>
                        <xs:documentation xml:lang="pl">Liczba artykułów naukowych w tym wydaniu.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:complexType>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
</xs:schema>